{
  "name": "order-api",
  "version": "1.0.0",
  "description": "Straight out of the tscript book for restfulness",
  "main": "./dist/server.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf ./dist && rimraf reports",
    "lint": "tslint ./src/**/*.ts ./test/**/*.spec.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.spec.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test yarn build && yarn lint",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --compilers ts:ts-node/register test/**/*.spec.ts --exit",
    "test:mutation": "stryker run",
    "stryker:init": "stryker init",
    "dev": "cross-env PORT=3030 NODE_ENV=dev ts-node ./src/server.ts",
    "prod": "PORT=3030 NODE_ENV=prod yarn build && yarn start",
    "start": "NODE_ENV=prod node dist/server.js",
    "tsc": "tsc"
  },
  "license": "MIT",
  "author": "Barrett Long - Studio Birdcage",
  "keywords": [
    "order POC",
    "Hands on RESTful Web Services with TypeScript 3",
    "TypeScript 3"
  ],
  "dependencies": {
    "@types/node": "^13.5.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-winston": "^4.0.2",
    "js2xmlparser": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.9",
    "mongoose-unique-validator": "^2.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "mocha": "^5.2.0",
    "rimraf": "^3.0.0",
    "stryker": "^0.35.1",
    "stryker-api": "^0.24.1",
    "stryker-html-reporter": "^0.18.1",
    "stryker-mocha-framework": "^0.15.1",
    "stryker-mocha-runner": "^0.17.1",
    "stryker-typescript": "^0.18.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.7",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.2",
    "@types/express-winston": "^4.0.0",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.14",
    "@types/mongoose": "^5.5.43",
    "@types/mongoose-unique-validator": "^1.0.3",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/winston": "^2.4.4"
  }
}
